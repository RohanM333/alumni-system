<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="312.0" prefWidth="529.0" style="-fx-border-color: #454545; -fx-border-width: 2px;" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="alumni.BasicEditController">
   <children>
      <AnchorPane layoutX="12.0" layoutY="9.0" prefHeight="299.0" prefWidth="518.0">
         <children>
            <Label layoutX="14.0" layoutY="14.0" text="Edit Contact Details" textFill="#a1a1a1">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
            <JFXTextField fx:id="mobEdit" labelFloat="true" layoutX="30.0" layoutY="59.0" prefHeight="31.0" prefWidth="198.0" promptText="Mobile">
               <font>
                  <Font size="15.0" />
               </font>
            </JFXTextField>
            <JFXTextField fx:id="landEdit" labelFloat="true" layoutX="283.0" layoutY="59.0" prefHeight="31.0" prefWidth="198.0" promptText="Landline">
               <font>
                  <Font size="15.0" />
               </font>
            </JFXTextField>
            <JFXTextField fx:id="emailEdit" labelFloat="true" layoutX="30.0" layoutY="125.0" prefHeight="31.0" prefWidth="198.0" promptText="Email">
               <font>
                  <Font size="15.0" />
               </font>
            </JFXTextField>
            <JFXTextField fx:id="linkedinEdit" labelFloat="true" layoutX="283.0" layoutY="125.0" prefHeight="31.0" prefWidth="198.0" promptText="Linkedin">
               <font>
                  <Font size="15.0" />
               </font>
            </JFXTextField>
            <JFXTextField fx:id="facebookEdit" labelFloat="true" layoutX="30.0" layoutY="196.0" prefHeight="31.0" prefWidth="198.0" promptText="Facebook">
               <font>
                  <Font size="15.0" />
               </font>
            </JFXTextField>
            <JFXTextField fx:id="instagramEdit" labelFloat="true" layoutX="283.0" layoutY="196.0" prefHeight="31.0" prefWidth="198.0" promptText="Instagram">
               <font>
                  <Font size="15.0" />
               </font>
            </JFXTextField>
            <JFXButton fx:id="saveContactbtn" alignment="CENTER_RIGHT" layoutX="400.0" layoutY="246.0" onAction="#onContactSaveBtn" prefHeight="40.0" prefWidth="80.0" style="-fx-text-fill: blue;" text="Save" textAlignment="RIGHT">
               <font>
                  <Font size="15.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="cancelContactbtn" alignment="CENTER_RIGHT" layoutX="320.0" layoutY="246.0" onMouseClicked="#onContactCancelBtn" prefHeight="40.0" prefWidth="80.0" style="-fx-text-fill: blue;" text="Cancel">
               <font>
                  <Font size="15.0" />
               </font>
            </JFXButton>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
